
  <h5>Create A Product to start receiving payment</h5>
  <form action="/products" method="POST" data-turbo="false">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <label for="name">Name Of product</label><br>
    <input type="text" id="name" name="product[name]" value=""><br>
    <label for="description">Description</label><br>
    <input type="text" id="description" name="product[description]" value=""><br>
    <input type="submit" name="commit" value="Get Started">
  </form>
